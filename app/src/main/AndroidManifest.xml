<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tripzoadmin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/tripzologo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/tripzologo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CustomerProfileActivity"></activity>
        <activity android:name=".DriverProfileActivity" />
        <activity android:name=".AdminMainActivity" />
        <activity android:name=".AdminRegisterActivity" />
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>